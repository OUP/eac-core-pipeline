<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5" id="WebApp_1">
  
  <welcome-file-list>
    <welcome-file>
			Login.jsp
		</welcome-file>
  </welcome-file-list>
<servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.eac.diagnostic.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>TestEACResponse</servlet-name>
    <servlet-class>com.eac.diagnostic.TestEACResponse</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>TestEACResponse</servlet-name>
    <url-pattern>/TestEACResponse</url-pattern>
</servlet-mapping>


<servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.eac.diagnostic.LogoutServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
</servlet-mapping>
<session-config>
  <session-timeout>20</session-timeout>
</session-config>

  
  <error-page>
		<error-code>403</error-code>
		<location>/static/404.html</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/static/404.html</location>
	</error-page>
	
	<error-page>
		<error-code>500</error-code>
		<location>/static/500.html</location>
	</error-page>
  
</web-app>


